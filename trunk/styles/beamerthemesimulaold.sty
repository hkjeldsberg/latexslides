\ProvidesPackage{beamerthemesimula}

% Combination of the Darmstadt and shadow themes

\mode<presentation>
%\setbeamertemplate{titlepage}
%
%}
%\setbeamertemplate{headline}[default]
\useoutertheme{smoothbars}
\useinnertheme[shadow=true]{rounded}
\usecolortheme{orchid}
\usecolortheme{whale}
\useoutertheme{simulashadow}
\usefonttheme[onlysmall]{structurebold}
% Suppress navigation symbols
\setbeamertemplate{navigation symbols}{}
%\logo{\includegraphics[height=0.15cm]{simula_logo_pos.eps}}
\setbeamercolor{title}{use=structure,fg=white,bg=structure.fg}
%\setbeamercolor{simula}{fg=white,bg=orange}
\setbeamercolor{simula}{fg=black,bg=white}
\setbeamerfont{block title}{size={}}
%Setting the following to anything (true,false,foo,bar) turns on pagenumbering
\def\@pagenumbering{}
\mode
<all>
